{
    "version": 2,
    "github": {
        "enabled": false
    },
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder",
            "config": {
                "serverFiles": [
                    "api/**",
                    "package.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "continue": true,
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate",
                "Service-Worker-Allowed": "/"
            }
        }
    ]
}